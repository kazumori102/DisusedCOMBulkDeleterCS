<Window x:Class="DisusedCOMBulkDeleterCS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DisusedCOMBulkDeleterCS"
        mc:Ignorable="d"
        Title="COM削除" Height="300" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="193*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Scan" Content="再スキャン" HorizontalAlignment="Left" Height="37" Margin="14,34,0,0" VerticalAlignment="Top" Width="69" Click="clk_btn_scan" Grid.Column="1"/>
        <Button x:Name="Delete" Content="削除" HorizontalAlignment="Left" Height="37" Margin="14,123,0,0" VerticalAlignment="Top" Width="69" Click="clk_btn_delete" Grid.Column="1"/>
        <Button x:Name="DeleteAll" Content="全て削除" HorizontalAlignment="Left" Height="37" Margin="14,201,0,0" VerticalAlignment="Top" Width="69" Click="clk_btn_delete_all" Grid.Column="1"/>
        <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="204" Margin="122,34,0,0" VerticalAlignment="Top" Width="613" AutoGenerateColumns="True" SelectionMode="Extended" SelectionUnit="FullRow" CanUserAddRows="False" Grid.Column="1" />

    </Grid>
</Window>
